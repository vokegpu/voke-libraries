--type library
--tag ekg
--target clang-msvc64 clang-mingw64 clang64
--url https://github.com/vokegpu/ekg-ui-library.git
--git-clone-args "--depth 64"
--include-dirs ./include/ekg
--binary-win-32-dirs ./lib/windows/
--binary-win-64-dirs ./lib/windows/
--binary-linux-64-dirs ./lib/linux/
--binary-linux-32-dirs ./lib/linux/
--run "cmake -S . -B ./cmake-build -G Ninja -D CMAKE_CXX_COMPILER=$cpp && cmake --build ./cmake-build"
